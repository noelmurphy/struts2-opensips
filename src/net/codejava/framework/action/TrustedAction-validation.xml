"http://struts.apache.org/dtds/xwork-validator-1.0.3.dtd">

<validators>

  <field name="src_ip">
      <field-validator type="requiredstring">
           <message >Source IP is required.</message>
      </field-validator>
        
  </field>
  <field name="proto">
      <field-validator type="requiredstring">
         <message>Protocol is required.</message>
     </field-validator>
  </field>

</validators>